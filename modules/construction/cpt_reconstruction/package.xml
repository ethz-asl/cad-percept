<?xml version="1.0"?>
<package format="2">
  <name>cpt_reconstruction</name>
  <version>0.1.0</version>
  <description>
    ROS-Module to reconstruct missing elements in digital building models.
  </description>

  <author email="phiegg@student.ethz.ch">Philipp Egg</author>
  <maintainer email="phiegg@student.ethz.ch">Philipp Egg</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <buildtool_depend>catkin_simple</buildtool_depend>

  <test_depend>rosunit</test_depend>

  <depend>cgal_catkin</depend>
  <depend>geometric_shapes</depend>
  <depend>glog_catkin</depend>
  <depend>gtsam_catkin</depend>
  <depend>pcl_ros</depend>
  <depend>roscpp</depend>
  <depend>tf_conversions</depend>
  <depend>eigen_catkin</depend>
  <depend>libnabo</depend>
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>std_msgs</depend>
  <depend>message_generation</depend>

  <depend>cgal_conversions</depend>
  <depend>cgal_visualizations</depend>
  <depend>cpt_utils</depend>
  <depend>cgal_definitions</depend>
  <depend>cgal_msgs</depend>

  <exec_depend>message_runtime</exec_depend>

</package>
